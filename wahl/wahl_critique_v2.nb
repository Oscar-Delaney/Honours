Cell["\<\
Find the expected number of mutants that will occur and go onto fix in one \
growth period, conditional on them having selective benefit s.\
\>", "Text",
 CellChangeTimes->{{3.8961381622342496`*^9, 3.8961381677732086`*^9}, {
  3.896209646278036*^9, 3.8962096493820252`*^9}, {3.8962097189120226`*^9, 
  3.896209727815095*^9}, {3.898718607148509*^9, 
  3.8987186679602194`*^9}},ExpressionUUID->"c7721632-1b5b-4fa5-b099-\
b64b924a2bbd"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"gamma", "=", 
  RowBox[{"Integrate", "[", 
   RowBox[{"integrand", ",", 
    RowBox[{"{", 
     RowBox[{"t", ",", "0", ",", "tau"}], "}"}], ",", 
    RowBox[{"Assumptions", "->", 
     RowBox[{"{", 
      RowBox[{
       RowBox[{"r", ">", "0"}], ",", 
       RowBox[{"d", ">", "0"}], ",", 
       RowBox[{"d", "<", "1"}], ",", 
       RowBox[{"t", ">", "0"}], ",", 
       RowBox[{"s", ">", "0"}]}], "}"}]}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.8961381622342496`*^9, 3.8961381677732086`*^9}, {
   3.896209646278036*^9, 3.8962096493820252`*^9}, {3.8962097189120226`*^9, 
   3.896209727815095*^9}, 3.898718607148509*^9, {3.8987192739683495`*^9, 
   3.8987192745955153`*^9}},
 CellLabel->"In[14]:=",ExpressionUUID->"7d15eb1d-96ba-4c4c-acc1-a1a80eaa0c60"],

Cell[BoxData[
 RowBox[{"-", 
  RowBox[{
   FractionBox["1", 
    RowBox[{"Log", "[", "d", "]"}]], 
   RowBox[{"r", " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"Hypergeometric2F1", "[", 
       RowBox[{"1", ",", 
        FractionBox["1", 
         RowBox[{"1", "+", "s"}]], ",", 
        RowBox[{"1", "+", 
         FractionBox["1", 
          RowBox[{"1", "+", "s"}]]}], ",", 
        RowBox[{"-", 
         FractionBox[
          RowBox[{
           RowBox[{"-", "1"}], "+", "d"}], 
          RowBox[{"d", " ", 
           RowBox[{"(", 
            RowBox[{
             RowBox[{"-", "1"}], "+", 
             SuperscriptBox["d", "s"]}], ")"}]}]]}]}], "]"}], "-", 
      RowBox[{"d", " ", 
       RowBox[{"Hypergeometric2F1", "[", 
        RowBox[{"1", ",", 
         FractionBox["1", 
          RowBox[{"1", "+", "s"}]], ",", 
         RowBox[{"1", "+", 
          FractionBox["1", 
           RowBox[{"1", "+", "s"}]]}], ",", 
         RowBox[{"-", 
          FractionBox[
           RowBox[{
            RowBox[{"(", 
             RowBox[{
              RowBox[{"-", "1"}], "+", "d"}], ")"}], " ", 
            SuperscriptBox["d", "s"]}], 
           RowBox[{
            RowBox[{"-", "1"}], "+", 
            SuperscriptBox["d", "s"]}]]}]}], "]"}]}]}], ")"}]}]}]}]], "Output",
 CellChangeTimes->{3.8961382215544505`*^9, 3.896209670492044*^9, 
  3.896209995867527*^9, 3.8987200671469812`*^9},
 CellLabel->"Out[14]=",ExpressionUUID->"53468325-63bc-425c-b926-1376064f9fa2"]
}, Open  ]],

Cell["\<\
Note that there is no exact closed form integral available over values of s.\
\>", "Text",
 CellChangeTimes->{{3.896138276548036*^9, 3.896138319104971*^9}, {
  3.8961384241385593`*^9, 3.8961384698515844`*^9}, {3.8962097393035088`*^9, 
