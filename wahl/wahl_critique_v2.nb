Cell["\<\
Find the rate of mutations occurring at time t and selective benefit s that \
will eventually fix (we ignore the population size and mutation rate here, \
the fixation rate is simply proportional to their product).\
\>", "Text",
 CellChangeTimes->{{3.896137834573684*^9, 3.8961378367754793`*^9}, {
  3.8961379841616163`*^9, 3.8961380516501975`*^9}, {3.8961381164888153`*^9, 
  3.8961381168217096`*^9}, {3.896209582091242*^9, 3.896209634781932*^9}, {
  3.898718500013583*^9, 3.898718589577385*^9}, {3.8987186204747396`*^9, 
  3.8987186207098722`*^9}, {3.898718673195758*^9, 
  3.898718675408102*^9}},ExpressionUUID->"92980f77-137e-4a29-abf1-\
4fe12338aa35"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"integrand", " ", "=", " ", 
  RowBox[{"Simplify", "[", 
   RowBox[{"d", " ", "r", " ", 
    RowBox[{"Exp", "[", 
     RowBox[{"r", " ", "t"}], "]"}], 
    RowBox[{
     RowBox[{"(", 
      RowBox[{"1", "-", "v"}], ")"}], "/", "tau"}]}], "]"}]}]], "Input",
 CellChangeTimes->{{3.896137834573684*^9, 3.8961378367754793`*^9}, {
   3.8961379841616163`*^9, 3.8961380516501975`*^9}, {3.8961381164888153`*^9, 
   3.8961381168217096`*^9}, {3.896209582091242*^9, 3.896209634781932*^9}, 
   3.898718500013583*^9},
 CellLabel->"In[13]:=",ExpressionUUID->"a848a53c-91a1-42b8-8412-9cf6e576326e"],

Cell[BoxData[
 RowBox[{"-", 
  FractionBox[
   RowBox[{"d", " ", 
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", "1"}], "+", 
      SuperscriptBox["d", "s"]}], ")"}], " ", 
    SuperscriptBox["\[ExponentialE]", 
     RowBox[{"r", " ", "t"}]], " ", 
    SuperscriptBox["r", "2"]}], 
   RowBox[{
    RowBox[{"(", 
     RowBox[{
      RowBox[{"-", "1"}], "+", 
      RowBox[{
       SuperscriptBox["d", 
        RowBox[{"1", "+", "s"}]], " ", 
       SuperscriptBox["\[ExponentialE]", 
        RowBox[{"r", " ", 
         RowBox[{"(", 
          RowBox[{"1", "+", "s"}], ")"}], " ", "t"}]]}], "-", 
      RowBox[{
       SuperscriptBox["d", "s"], " ", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"-", "1"}], "+", 
         SuperscriptBox["\[ExponentialE]", 
          RowBox[{"r", " ", 
           RowBox[{"(", 
            RowBox[{"1", "+", "s"}], ")"}], " ", "t"}]]}], ")"}]}]}], ")"}], 
    " ", 
    RowBox[{"Log", "[", "d", "]"}]}]]}]], "Output",
