Cell[BoxData[
 RowBox[{
  SubsuperscriptBox["\[Integral]", "0", "\[Infinity]"], 
  RowBox[{
   RowBox[{"-", 
    RowBox[{
     FractionBox["1", 
      RowBox[{"Log", "[", "d", "]"}]], 
     RowBox[{"a", " ", 
      SuperscriptBox["\[ExponentialE]", 
       RowBox[{
        RowBox[{"-", "a"}], " ", "s"}]], " ", "r", " ", 
      RowBox[{"(", 
       RowBox[{
        RowBox[{"Hypergeometric2F1", "[", 
