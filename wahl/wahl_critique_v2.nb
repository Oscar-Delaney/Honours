78d03d4b704f"],

Cell[CellGroupData[{

Cell[BoxData[
 RowBox[{"Integrate", "[", 
  RowBox[{
   RowBox[{"gamma", "*", "a", "*", 
    RowBox[{"Exp", "[", 
     RowBox[{
      RowBox[{"-", "a"}], "*", "s"}], "]"}]}], ",", 
   RowBox[{"{", 
    RowBox[{"s", ",", "0", ",", "Infinity"}], "}"}]}], "]"}]], "Input",
 CellChangeTimes->{{3.896138276548036*^9, 3.896138319104971*^9}, {
